	call TriggerAddCondition(gg_trg_B_Reworked_Mark_P1,Condition(function Trig_B_Reworked_Mark_P1_Conditions))
	call TriggerAddAction(gg_trg_B_Reworked_Mark_P1,function Trig_B_Reworked_Mark_P1_Actions)
