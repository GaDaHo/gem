	set udg_CountLivesLost[6]=(udg_CountLivesLost[6]-GetUnitPointValue(GetTriggerUnit()))
	call RemoveUnit(GetTriggerUnit())
	call PlaySoundBJ(gg_snd_SpellShieldImpact1)
