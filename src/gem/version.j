// # Gem Version

globals
	constant integer Gem_Version__MAJOR = 1
	constant integer Gem_Version__MINOR = 4
	constant integer Gem_Version__PATCH = 0

	constant string Gem_Version__EXTRA = "-alpha"

	// This flag does nothing within the JASS scripts. It specifies that
	// external metadata should be appended to the version string.
	constant boolean Gem_Version__METADATA = true
endglobals
